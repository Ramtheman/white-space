<div class="container mt-5">
  <h1><%= current_user.first_name %>'s Bookings:</h1>
    <% @bookings.each do |booking| %>
    <h4>Space: <%= booking.space.title %></h4>
    <p>Type: <%= booking.space.space_type %></p>
    <p>Price: £<%= booking.space.price %></p>
    <p>Owner: <%= booking.space.first_name %> <%= booking.space.last_name %></p>
    <p>From: £<%= booking.start_date %></p>
    <p>To: £<%= booking.end_date %></p>
  <% end %>
  <%= link_to "Back to home", root_path, class: 'btn btn-primary' %>
</div>
